{
  "name": "ozonep-isomorphic-git",
  "version": "0.0.4",
  "description": "A pure JavaScript reimplementation of git for node and browsers",
  "typings": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "browser": "./dist/index.js",
  "sideEffects": false,
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "nps",
    "format": "eslint --fix ./src",
    "prettify": "prettier --write ./src",
    "build": "npm run build.rollup && npm run build.typings && npm run build.webpack",
    "build.rollup": "rollup -c --no-treeshake",
    "build.typings": "tsc -p declaration.tsconfig.json && cp index.d.ts index.umd.min.d.ts",
    "build.webpack": "webpack",
    "build.treeshake": "agadoo"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/isomorphic-git/isomorphic-git.git"
  },
  "keywords": [
    "git",
    "isomorphic"
  ],
  "author": "William Hilton <wmhilton@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/isomorphic-git/isomorphic-git/issues"
  },
  "homepage": "https://isomorphic-git.org/",
  "files": [
    "dist/*",
    "http/*"
  ],
  "dependencies": {
    "async-lock": "^1.2.8",
    "clean-git-ref": "^2.0.1",
    "diff3": "0.0.3",
    "ignore": "^5.1.8",
    "pako": "^2.0.3",
    "readable-stream": "^3.6.0",
    "sha1-uint8array": "^0.10.0"
  },
  "devDependencies": {
    "@cerner/duplicate-package-checker-webpack-plugin": "2.1.0",
    "@isomorphic-git/cors-proxy": "2.7.0",
    "@isomorphic-git/pgp-plugin": "0.0.7",
    "@types/node": "14.14.41",
    "agadoo": "2.0.0",
    "buffer-lite": "^1.0.0",
    "diff-lines": "1.1.1",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-prettier-standard": "^4.0.1",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "git-http-mock-server": "2.0.0",
    "path-browserify": "^1.0.1",
    "prettier": "^2.2.1",
    "prettier-config-standard": "^4.0.0",
    "pretty-format": "26.6.2",
    "process": "0.11.10",
    "rollup": "^2.45.2",
    "standard": "16.0.3",
    "typescript": "4.2.4",
    "webpack": "^5.35.1",
    "webpack-cli": "^4.6.0"
  }
}
