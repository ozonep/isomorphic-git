{
  "name": "isomorphic-git",
  "version": "0.0.0-development",
  "description": "A pure JavaScript reimplementation of git for node and browsers",
  "typings": "./index.d.ts",
  "main": "./index.js",
  "module": "./index.js",
  "unpkg": "./index.umd.min.js",
  "bin": {
    "isogit": "./cli.cjs"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "nps",
    "format": "nps format",
    "build": "npm run build.rollup && npm run build.typings && npm run build.webpack && npm run build.treeshake",
    "build.rollup": "rollup -c --no-treeshake",
    "build.typings": "tsc -p declaration.tsconfig.json && cp index.d.ts index.umd.min.d.ts",
    "build.webpack": "webpack",
    "build.treeshake": "agadoo"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/isomorphic-git/isomorphic-git.git"
  },
  "keywords": [
    "git",
    "isomorphic"
  ],
  "author": "William Hilton <wmhilton@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/isomorphic-git/isomorphic-git/issues"
  },
  "homepage": "https://isomorphic-git.org/",
  "files": [
    "cli.cjs",
    "browser-tests.json",
    "http/*",
    "index.cjs",
    "index.d.ts",
    "index.js",
    "index.umd.min.js",
    "index.umd.min.d.ts",
    "index.umd.min.js.map",
    "size_report.html"
  ],
  "dependencies": {
    "async-lock": "^1.2.8",
    "clean-git-ref": "^2.0.1",
    "crc-32": "^1.2.0",
    "diff3": "0.0.3",
    "ignore": "^5.1.8",
    "minimisted": "^2.0.1",
    "pako": "^2.0.3",
    "pify": "^5.0.0",
    "readable-stream": "^3.6.0",
    "sha.js": "^2.4.11"
  },
  "devDependencies": {
    "@babel/core": "7.13.15",
    "@babel/plugin-transform-async-to-generator": "7.13.0",
    "@babel/preset-env": "7.13.15",
    "@babel/runtime": "7.13.10",
    "@cerner/duplicate-package-checker-webpack-plugin": "2.1.0",
    "@isomorphic-git/cors-proxy": "2.7.0",
    "@isomorphic-git/pgp-plugin": "0.0.7",
    "@types/jest": "24.0.18",
    "@types/node": "14.14.37",
    "agadoo": "2.0.0",
    "all-contributors-cli": "6.20.0",
    "babel-loader": "8.2.2",
    "browserfs": "2.0.0",
    "bundlewatch": "0.2.7",
    "cross-env": "6.0.0",
    "decompress": "^4.2.0",
    "diff-lines": "1.1.1",
    "envify": "4.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-prettier-standard": "^3.0.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "git-http-mock-server": "2.0.0",
    "github-comment": "1.0.1",
    "inquirer": "^7.0.0",
    "jasmine-core": "3.4.0",
    "jest": "24.9.0",
    "jest-fixtures": "wmhilton-contrib/jest-fixtures#win32",
    "jest-junit": "7.0.0",
    "jsdoc-api": "5.0.3",
    "markdown-table": "1.1.3",
    "prettier": "^1.19.1",
    "prettier-config-standard": "^1.0.1",
    "pretty-format": "24.9.0",
    "puppeteer": "2.0.0",
    "replace-in-file": "4.1.3",
    "rollup": "^1.32.1",
    "rollup-plugin-node-resolve": "5.2.0",
    "standard": "13.1.0",
    "timeout-cli": "0.3.2",
    "typescript": "3.9.2",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12"
  },
  "bundlewatch": {
    "files": [
      {
        "path": "./index.umd.min.js",
        "maxSize": "100kb"
      }
    ],
    "ci": {
      "trackBranches": [
        "main"
      ]
    }
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/isomorphic-git",
    "logo": "https://opencollective.com/isomorphic-git/logo.txt"
  }
}
